{
  "name": "exam-generator-server",
  "version": "1.0.0",
  "scripts": {
    "start": "node -r dotenv/config dist/server.js",
    "dev": "nodemon -r dotenv/config --exec ts-node server.ts",
    "build": "tsc",
    "debug": "node --inspect-brk -r ts-node/register server.ts",
    "debug:watch": "nodemon --inspect -r ts-node/register server.ts"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.47.14",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "mammoth": "^1.6.0",
    "multer": "^1.4.5-lts.1",
    "officeparser": "^5.1.1",
    "openai": "^4.78.1",
    "pdf-parse": "^1.1.1",
    "textract": "^2.5.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.11.0",
    "@types/pdf-parse": "^1.1.4",
    "@types/textract": "^2.4.5",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
